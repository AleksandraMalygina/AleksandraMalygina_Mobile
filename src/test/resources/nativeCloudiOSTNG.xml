    <parameter name="udid" value="3e50758dd2e1179a014ee7b51dc4285d73118107"></parameter>
<!--    iPhone 8 Plus iOS 12.4-->
    <parameter name="platformName" value="iOS"></parameter>
    <parameter name="bundleId" value="com.epam.EPAMTestApp.ii"></parameter>
    <parameter name="deviceLocation" value="remote"></parameter>
    <test name="Native Cloud iOS tests">
        <groups>
            <run>
                <include name="native"/>
