<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Web Cloud Android mobile testing Suite">
    <parameter name="appType" value="web"></parameter>
<!--    <parameter name="deviceName" value="GOOGLE Pixel 2 XL"></parameter>-->
    <parameter name="udid" value="802KPFX1550899"></parameter>
    <parameter name="platformName" value="Android"></parameter>
    <parameter name="browserName" value="Chrome"></parameter>
    <parameter name="deviceLocation" value="remote"></parameter>
    <test name="Web Cloud Android tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>